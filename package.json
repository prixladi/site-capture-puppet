  "scripts": {
    "dev": "nodemon ./src/index.ts",
    "build": "tsc",
    "start": "node ./dist/index.js",
    "eslint:check": "eslint -c .eslintrc.json \"**/*.{js,ts}\"",
    "eslint:fix": "eslint -c .eslintrc.json --fix  \"**/*.{js,ts}\"",
    "format:check": "prettier -l \"**/*.{js,ts}\"",
