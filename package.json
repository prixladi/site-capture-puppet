    "stream-buffers": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.14",
    "@types/puppeteer": "^5.4.2",
    "@types/stream-buffers": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.16.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
